{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": ["error", "double", { "allowTemplateLiterals": true }],
    "comma-dangle": "off",
    "object-curly-spacing": ["error", "always"],
    "require-jsdoc": "off"
  },
  "extends": ["eslint:recommended", "google"]
}